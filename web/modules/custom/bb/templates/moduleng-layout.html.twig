{{ drupal_view('infos_module', 'block_1') }}

{% if 'administrator' in user.roles or 'd_cons' in user.roles %}
<details open class="details-wrapper" id="journal">
  <summary class="journal-dark">Zone Conseillers - Non visible par les gestionnaires et inspecteurs</summary>
  <div class="common-wrapper journal">
    <div class="pure-g">
      <div class="pure-u-17-24">
        {{ html.journal }}
        <br>
        {{ html.prioritaire }}
        {{ html.reforme }}
        {{ html.report }}
      </div>
      <div class="pure-u-1-24">
        &nbsp;
      </div>
      <div class="pure-u-6-24">
        {{ html.cfile }}
      </div>
    </div>
    <div class="clear"></div>
  </div>
</details>
{% endif %}

{% if 'administrator' in user.roles or 'd_cons' in user.roles %}
{% if html.isInspecOpen == 1 %}
<details open class="details-wrapper" id="journal">
  <summary class="inspec-dark">Zone d'échanges entre Inspecteurs et Conseillers. - Dernière modification : {{ html.last_mod}} par {{ html.last_user }}</summary>
{% else %}
<details class="details-wrapper" id="journal">
  <summary class="inspec-dark">Zone d'échanges entre Inspecteurs et Conseillers. - Dernière modification : {{ html.last_mod}} par {{ html.last_user }}</summary>
{% endif %}
  <div class="common-wrapper inspec">
    <div class="pure-g">
      <div class="pure-u-17-24">
        {{ html.journalinspec }}
      </div>
      <div class="pure-u-1-24">
        &nbsp;
      </div>
      <div class="pure-u-6-24">
        {{ html.ifile }}
      </div>
    </div>
    <div class="clear"></div>
  </div>
</details>
{% endif %}

{% if 'administrator' in user.roles or 'd_admi' in user.roles or 'd_de' in user.roles %}
{% if 'administrator' in user.roles %}
<details class="details-wrapper" id="sessions-x">
{% endif %}
{% if 'd_admi' in user.roles or 'd_de' in user.roles%}
<details open class="details-wrapper" id="sessions">
{% endif %}
  <summary class="sessions-dark">Zone Gestionnaires - Non visible par les Conseillers. - Dernière modification : {{ html.last_mod}} par {{ html.last_user }}</summary>
  <div class="common-wrapper sessions">
    <div class="pure-g">
      <!--<div class="pure-u-13-24">-->
      <div class="pure-u-24-24">
        <p><b> Sessions stagiaires </b></p>
        {{ drupal_view('sessions_d_un_module', 'block_2') }}
      </div>
      <!--<div class="pure-u-1-24"> &nbsp; </div>
      <div class="pure-u-10-24">-->
     {% if 'd_admi' in user.roles %}
      <div class="pure-u-24-24">
        <p><b> Sessions formateurs </b></p>
        {{ drupal_view('sessions_d_un_module', 'block_3') }}
      </div>
     {% endif %}
    </div>
    <div class="clear"></div>
  </div>
</details>
{% endif %}

{% if 'administrator' in user.roles or 'd_inspec' in user.roles %}
{% if 'administrator' in user.roles %}
<details class="details-wrapper" id="sessions-y">
{% endif %}
{% if 'd_inspec' in user.roles %}
<details open class="details-wrapper" id="sessions-y">
{% endif %}
  <summary class="inspec-dark">Zone d'échanges entre Inspecteurs et Conseillers. - Dernière modification : {{ html.last_mod}} par {{ html.last_user }}</summary>
  <div class="common-wrapper inspec">
    {{ drupal_view('sessions_d_un_module', 'block_4') }}
    <div class="clear"></div>
      <div class="pure-u-16-24">
        {{ html.journalinspec }}
      </div>
      <div class="pure-u-1-24">
        &nbsp;
      </div>
      <div class="pure-u-6-24">
        {{ html.ifile }}
      </div>
    </div>
    <div class="clear"></div>
  </div>
</details>
{% endif %}



{% if 'd_admi' in user.roles or 'd_cons' in user.roles %}
<details open class="details-wrapper" id="sessions">
