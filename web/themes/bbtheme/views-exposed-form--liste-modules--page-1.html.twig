{#
/**
 * @file
 * Default theme implementation of a views exposed form.
 *
 * Available variables:
 * - form: A render element representing the form.
 *
 * @see template_preprocess_views_exposed_form()
 *
 * @ingroup themeable
 */
#}
{% if q is not empty %}
  {#
    This ensures that, if clean URLs are off, the 'q' is added first,
    as a hidden form element, so that it shows up first in the POST URL.
  #}
{{ q }}
{% endif %}

<div class="pure-g">
    <div class="pure-u-1-6">
      {{form.id_disp}}
    </div>
    <div class="pure-u-1-4">
      {{form.resp_filter}}
    </div>
    <div class="pure-u-1-3">
      {{form.actions}}
    </div>
</div>

<details class="form-wrapper" id="searchcrit">
<summary>Options de recherche suppl√©mentaires</summary>

   <div class="pure-g">
{% for k,v in form %}
  {% if (k not in ['id_disp','resp_filter','#action', '#id', '#form_id', '#type', '#defaults_loaded', '#method', '#validated', '#title_display', '#description_display', '#build_id','actions', '#sorted', '#render_children']) %}
    <div class="pure-u-1-3"> {{v}} </div>
{%endif%}
{%endfor%}
</div>

</details>

{# {{ form }} #}
