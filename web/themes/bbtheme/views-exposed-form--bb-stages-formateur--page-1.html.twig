{% if q is not empty %}
  {#
    This ensures that, if clean URLs are off, the 'q' is added first,
    as a hidden form element, so that it shows up first in the POST URL.
  #}
{{ q }}
{% endif %}

{% set principal = ['id_disp','actions'] %}
{% set small = [] %}
{% set large = [] %}
{% set unused = ['#action', '#id', '#form_id', '#type', '#defaults_loaded', '#method', '#validated', '#title_display', '#description_display', '#build_id','actions', '#sorted', '#render_children'] %}
{% set trash = unused|merge(small)|merge(large)|merge(principal) %}

<div class="pure-g">
  {% for k,v in form %}
    {% if (k not in trash) %}
      {{v}}
    {%endif%}
  {%endfor%}
      <div class="pure-u-1-12"> {{form.id_disp}} </div>
      <div class="pure-u-1-12"> {{form.actions}} </div>
</div>
</div>

