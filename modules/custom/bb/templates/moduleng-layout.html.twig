{% if "REMOVEME_administrator" in user.roles %}
{{ kint() }}
{% endif %}

{{ drupal_view('infos_module', 'block_1') }}


{% if 'd_cons' in user.roles %}
<details open class="details-wrapper" id="journal">
  <summary class="journal-dark">Zone Conseillers - Non visible par les administratifs</summary>
  <div class="common-wrapper journal">
    <div class="pure-g">
      <div class="pure-u-17-24">
        {{ _context[''].journal }}
      </div>
      <div class="pure-u-1-24">
        &nbsp;
      </div>
      <div class="pure-u-6-24">
        <hr/>
        {{ _context[''].propfile }}
        <hr/>
        {{ _context[''].cfile }}
        <hr/>
      </div>
    </div>
    <div class="clear"></div>
  </div>
</details>

<details open class="details-wrapper" id="sessions">
  <summary class="sessions-dark">Zone CONSEILLERS-administratifs</summary>
  <div class="common-wrapper sessions">
    {{ drupal_view('sessions_d_un_module', 'block_2') }}
    <div class="pure-g">
      <div class="pure-u-10-24">
        {{ _context[''].infospasconvoc }}
      </div>
      <div class="pure-u-1-24"> &nbsp; </div>
      <div class="pure-u-5-24">
        {{ _context[''].afile }}
      </div>
      <div class="pure-u-1-24"> &nbsp; </div>
      <div class="pure-u-7-24">
        {{ _context[''].infossurconvoc }}
      </div>
    </div>
    <div class="clear"></div>
  </div>
</details>
{% endif %}

{% if 'd_admi' in user.roles %}
<details open class="details-wrapper" id="sessions">
  <summary class="sessions-dark">Zone Conseillers-ADMINISTRATIFS</summary>
  <div class="common-wrapper sessions">
    <div class="pure-g">
      <div class="pure-u-13-24">
        <p><b> Sessions formateurs </b></p>
        {{ drupal_view('sessions_d_un_module', 'block_3') }}
      </div>
      <div class="pure-u-1-24"> &nbsp; </div>
      <div class="pure-u-10-24">
        <p><b> Sessions stagiaires </b></p>
        {{ drupal_view('sessions_d_un_module', 'block_2') }}
      </div>
    </div>
    <div class="pure-g">
      <div class="pure-u-10-24">
        {{ _context[''].infospasconvoc }}
      </div>
      <div class="pure-u-1-24"> &nbsp; </div>
      <div class="pure-u-5-24">
        {{ _context[''].afile }}
      </div>
      <div class="pure-u-1-24"> &nbsp; </div>
      <div class="pure-u-7-24">
        {{ _context[''].infossurconvoc }}
      </div>
    </div>
    <div class="clear"></div>
  </div>
</details>
{% endif %}


